{
  "entryFile": "src/launcher.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["services/**/src/controllers/*.ts"],
  "spec": {
    "outputDirectory": "build",
    "specVersion": 3,
    "basePath": ""
  },
  "routes": {
    "middleware": "koa",
    "routesDir": "src/routes"
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    }
  }
}
